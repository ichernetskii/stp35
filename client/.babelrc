{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3",
        "modules": "auto"
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-optional-chaining",
    ["module-resolver", {
      "alias": {
        "@": "./src",
        "assets": "./src/assets",
        "images": "./src/images",
        "components": "./src/components"
      }
    }]
  ]
}
